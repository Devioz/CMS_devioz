<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <remove name="*.PHP" />
            <add name="PHP_via_FastCGI" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\Program Files (x86)\PHP\v7.4\php-cgi.exe" resourceType="File" />
        </handlers>
        <staticContent>
            <mimeMap fileExtension=".lang" mimeType="text/plain" />
        </staticContent>
    </system.webServer>
</configuration>
